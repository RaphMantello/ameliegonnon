<div class="gallery">
  <div class="side-bar">
    <div class="current-expo-title">
      <div class="arrow-down"></div>
      <%= link_to gallery_path, remote: true do %>
        <h3 id="expo-title-content"><%= @expo.name %></h3>
      <% end %>
      <div class="arrow-down"></div>
    </div>
    <div class="expo-titles">
      <% @expos.each do |expo| %>
        <a href="" data-remote="true" class="expo-title"><%= expo.name %></a>
      <% end %>
    </div>
  </div>
  <div class="gallery-content">
    <div class="under-nav"></div>
    <div class="container">
      <div class="title">
        <h2>gallery</h2>
      </div>
      <% @expo.artworks.each do |aw| %>
        <h3><%= aw.name %></h3>
      <% end %>
    </div>
  </div>
</div>

<%= javascript_pack_tag 'sidebar_expand' %>
